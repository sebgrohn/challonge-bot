{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": "sdb:ListDomains",
            "Effect": "Allow",
            "Resource": "*"
        },
        {
            "Action": "sdb:*",
            "Effect": "Allow",
            "Resource": "arn:aws:sdb:*:*:domain/*"
        }
    ]
}
